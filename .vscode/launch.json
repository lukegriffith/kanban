{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8081",
            "webRoot": "${workspaceRoot}",
            "sourceMaps": true
        },
        
        {
            "name": "Attach",
            "type": "chrome",
            "request": "attach",
            "url": "http://localhost:8081",
            "port": 9222,
            "webRoot": "${workspaceRoot}"
        }

    ]
}